export PATH="./bin:$HOME/.rbenv/shims:/usr/local/bin:/usr/local/sbin:$HOME/.sfs:$ZSH/bin:$PATH"

# Add iOS tools paths
export PATH="/usr/local/bin:$HOME/Developer/devtools/utilities:$PATH"
export PATH="/opt/local/bin:/opt/local/sbin:$PATH"

export MANPATH="/usr/local/man:/usr/local/mysql/man:/usr/local/git/man:$MANPATH"

# Add NPM packages to node path
export NODEPATH="/usr/local/lib/node_modules:$NODEPATH"

# Add Android Tools paths
export PATH="/Users/aecorre/android-sdks/tools:/Users/aecorre/android-sdks/platform-tools:$PATH"

# Add linkedin commands
export PATH="$PATH:/usr/local/linkedin/bin"

# Add npm bin
export PATH="$PATH:/usr/local/share/npm/bin"

# Add nvm bin to path
source ~/.nvm/nvm.sh
