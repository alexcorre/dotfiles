[user]
	name = Alex Corre
	email = alexcorre513@gmail.com
[push]
	default = simple
[core]
	excludesfile = /Users/acorre/.gitignore_global
	editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  sync = pull --rebase
[mergetool]
	keepBackup = false
[merge]
	tool = opendiff
[credential]
	helper = osxkeychain
